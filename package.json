{
  "name": "website-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "lint:nofix": "eslint . --ignore-path .gitignore",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "eslintConfig": {
    "extends": "ethang",
    "rules": {
      "max-lines-per-function": "off",
      "@typescript-eslint/member-ordering": "off"
    }
  },
  "dependencies": {
    "@ethang/hooks": "^1.5.21",
    "@ethang/use-form": "^1.6.53",
    "@ethang/util": "^3.1.1",
    "@faker-js/faker": "^8.4.1",
    "@heroicons/react": "^2.1.1",
    "@nextui-org/react": "^2.2.9",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@remix-run/serve": "^2.6.0",
    "@sanity/client": "^6.13.3",
    "@sanity/image-url": "^1.0.2",
    "color-convert": "^2.0.1",
    "convert": "^4.14.1",
    "framer-motion": "^11.0.5",
    "isbot": "^4.4.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^10.0.4",
    "tailwind-merge": "^2.2.1",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ethang/project-builder": "^1.1.20",
    "@remix-run/dev": "^2.6.0",
    "@remix-run/eslint-config": "^2.6.0",
    "@types/color-convert": "^2.0.3",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.56.0",
    "eslint-config-ethang": "^13.0.9",
    "tailwindcss": "^3.4.1",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3"
  }
